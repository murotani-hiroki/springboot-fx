<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrtn.fx.mybatis.repository.ext.CurrencyPairSearchMapper">
    <select id="findAll" resultType="com.mrtn.fx.mybatis.entity.CurrencyPair">
        SELECT
            id,
            currency_pair
        FROM
            currency_pair
        ORDER BY
            id
    </select>
</mapper>

